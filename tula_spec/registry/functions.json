  "functions": [
    {
      "id": "slot_validator",
      "version": "1.3.2",
      "hash": "ab12fe89c456def0123456789abcdef012345678",
      "description": "Валидатор слотов бронирования",
      "tags": ["booking", "validation", "slots"],
      "author": "JALM Foundation",
      "created_at": "2024-06-12T10:00:00Z",
      "updated_at": "2024-06-12T10:00:00Z",
      "input_schema": {
        "type": "object",
        "properties": {
          "slot": {
            "type": "object",
            "properties": {
              "datetime": {
                "type": "string",
                "format": "datetime",
                "description": "Дата и время слота"
              },
              "duration": {
                "type": "integer",
                "minimum": 15,
                "maximum": 480,
                "description": "Длительность в минутах"
              },
              "service_id": {
                "type": "string",
                "format": "uuid",
                "description": "ID услуги"
              }
            },
            "required": ["datetime", "duration", "service_id"]
          }
        },
        "required": ["slot"]
      },
      "output_schema": {
        "type": "object",
        "properties": {
          "slot_uuid": {
            "type": "string",
            "format": "uuid",
            "description": "UUID созданного слота"
          },
          "status": {
            "type": "string",
            "enum": ["valid", "invalid", "conflict"],
            "description": "Статус валидации"
          },
          "message": {
            "type": "string",
            "description": "Сообщение о результате"
          }
        },
        "required": ["slot_uuid", "status"]
      },
      "implementation": {
        "type": "python",
        "file": "slot_validator.py",
        "function": "create"
      },
      "dependencies": [],
      "runtime": {
        "timeout": 30,
        "memory_limit": "128MB",
        "retries": 3
      }
    },
    {
