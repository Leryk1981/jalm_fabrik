{
  "name": "jalm-ui-market",
  "version": "1.0.0",
  "description": "JALM Full Stack Market Place UI - веб-интерфейс для управления экосистемой",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "zustand": "^4.3.0",
    "axios": "^1.3.0",
    "recharts": "^2.5.0",
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "clsx": "^1.2.0",
    "date-fns": "^2.29.0",
    "react-query": "^3.39.0",
    "react-hook-form": "^7.43.0",
    "react-hot-toast": "^2.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.29.0",
    "@vitest/ui": "^0.29.0",
    "@vitest/coverage-v8": "^0.29.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "jalm",
    "marketplace",
    "react",
    "typescript",
    "dashboard",
    "ui",
    "frontend"
  ],
  "author": "JALM Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jalm-team/jalm-full-stack.git",
    "directory": "ui-market"
  },
  "bugs": {
    "url": "https://github.com/jalm-team/jalm-full-stack/issues"
  },
  "homepage": "https://github.com/jalm-team/jalm-full-stack#readme"
} 